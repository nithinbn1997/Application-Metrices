[MASTER]
# Specify the list of plugins (comma-separated) to load.
load-plugins=

[MESSAGES CONTROL]
# Disable specific error messages (e.g., C0114, C0301) by adding their codes here.
disable=C0114,C0301,C0303,C0304,C0411,C0412,C0103,C0116,C1802,W1401,W0621,W0707,C0121,W1309,W0105,R1705,E0401,W1514,W3101,C0305,W0718,W0613,C0115,R0903,W0611,E1101,W0212,W0104,W0612

[REPORTS]
# Set the output format (text, colorized, parseable, etc.).
output-format=colorized
